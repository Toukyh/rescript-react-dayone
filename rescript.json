{
  "name": "rescript-app",
  "sources": [
    {
      "dir": "src",
      "subdirs": true
    }
  ],
  "package-specs": [
    {
      "module": "esmodule",
      "in-source": true
    }
  ],
  "suffix": ".res.mjs",
  "bs-dependencies": [
    "@rescript/core",
    "@rescript/react",
    "rescript-json-schema",
    "rescript-schema",
    "rescript-webapi"
  ],
  "bsc-flags": [
    "-open RescriptCore",
    "-open RescriptSchema",
    "-open Webapi",
    "-open Promise"
  ],
  "jsx": {
    "version": 4
  }
}
